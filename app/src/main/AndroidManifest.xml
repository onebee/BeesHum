<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.one.bee">

    <application
        android:name=".HiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BeesHum">
        <activity android:name=".demo.refresh.HiRefreshDemoActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".demo.tab.HiTabTopActivity"></activity>
        <activity android:name=".demo.tab.HiTabBottomDemoActivity" />
        <activity android:name=".demo.HiLogDemoActivity" />
    </application>

</manifest>